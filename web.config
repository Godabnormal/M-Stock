<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <handlers accessPolicy="Read, Execute, Script">
            <add name="streamlit" path="*.py" verb="*" modules="FastCgiModule" scriptProcessor="C:\Users\Administrator\AppData\Local\Programs\Python\Python311\Scripts\streamlit.exe run main.py" resourceType="Either" requireAccess="Execute" />
        </handlers>
    </system.webServer>
</configuration>
